<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<HBox fx:id="paneCloseMethod" alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-info" text="y'(x):" />
      <TextField fx:id="txtDerivate" prefHeight="26.0" prefWidth="250.0">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </TextField>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-info" text="A:" />
      <TextField fx:id="txtPointAOpen" prefHeight="26.0" prefWidth="114.0" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-info" text="Error: ">
         <HBox.margin>
            <Insets left="20.0" />
         </HBox.margin>
      </Text>
      <TextField fx:id="txtError1" prefHeight="26.0" prefWidth="72.0" promptText="ej. 0.01">
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin>
      </TextField>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-info" text="\%" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </Text>
   </children>
   <padding>
      <Insets bottom="10.0" top="10.0" />
   </padding>
   <styleClass>
      <String fx:value="panel" />
      <String fx:value="panel-footer" />
   </styleClass>
</HBox>
